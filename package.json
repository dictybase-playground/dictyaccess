{
  "name": "dictyaccess",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.1.0",
    "circos": "^2.1.0",
    "dicty-components-header-footer":
      "github:dictyBase/dicty-components-header-footer",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-fontawesome": "^1.6.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "recharts": "^1.0.0-beta.10",
    "styled-components": "^2.4.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "flow-bin": "^0.72.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.11.1",
    "react-styleguidist": "^7.0.5",
    "react-test-renderer": "^16.3.2"
  }
}
